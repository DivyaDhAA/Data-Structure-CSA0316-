//String Validity
#include <stdio.h>
#include <string.h>
int main() {\Divaydharshini\nReg No:192324031\n");
    char str[100];
    printf("Enter String: ");
    scanf("%[^\n]s",str);
    int len=strlen(str);
    if(str[0]!='('&&str[len-1]!=')'){
    	printf("\nThe string is not valid");
	}
	else
	printf("\nThe string is valid");
    return 0;
}
